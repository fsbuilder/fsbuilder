# ==============================================
# FS Builder - Environment Configuration Example
# ==============================================
# Copy this file to .env and update values as needed
# For production, ensure all secrets are properly configured

# Database (PostgreSQL)
DATABASE_URL="postgresql://postgres:password@localhost:5432/fsbuilder?schema=public"

# JWT Authentication
JWT_SECRET="your-super-secret-jwt-key-change-in-production"
JWT_EXPIRES_IN="7d"

# Server Configuration
PORT=4000
NODE_ENV=development

# Client Configuration (Vite uses VITE_ prefix)
VITE_API_URL=http://localhost:4000/api
VITE_APP_NAME="FS Builder"
